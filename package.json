{
  "name": "@pomerium/js-sdk",
  "version": "1.1.0",
  "description": "Verify and Parse Pomerium JWTs",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/esm/types/index.d.ts",
      "default": "./lib/esm/index.mjs"
    }
  },
  "types": "./lib/esm/types/index.d.ts",
  "main": "./lib/esm/index.mjs",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && tsc -p ./configs/tsconfig.base.json && mv lib/esm/index.js lib/esm/index.mjs",
    "prepack": "npm run build",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pomerium/js-sdk.git"
  },
  "keywords": [
    "zero trust",
    "jwt",
    "verify",
    "pomerium"
  ],
  "author": "Nathan Hayfield",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "jose": "^4.15.5"
  }
}
